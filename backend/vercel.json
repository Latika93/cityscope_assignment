{
    "installCommand": "npm install",
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/nodejs"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}